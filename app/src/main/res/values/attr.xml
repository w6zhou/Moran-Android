    <!--自定义的滑动的circleview-->
    <declare-styleable name="ScrollCircleView">
        <!--内部的item的个数-->
        <attr name="itemNum" format="integer" />
        <!--内部的item的背景颜色-->
        <attr name="itemBg" format="color" />
        <!--内部的item的字体大小-->
        <attr name="itemTextSize" format="dimension" />
        <!--内部的item的宽度-->
        <attr name="itemWidth" format="dimension" />

        <!--内部的item的高度-->
        <attr name="itemHeight" format="dimension" />
        <!--自定义view距左侧的距离-->
        <attr name="scrollLeft" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="CircleIndicator">
        <attr name="ci_radius" format="dimension"/>
        <attr name="ci_margin" format="dimension"/>
        <attr name="ci_background" format="color"/>
        <attr name="ci_selected_background" format="color"/>
        <attr name="ci_gravity">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="ci_mode">
            <enum name="inside" value="0"/>
            <enum name="outside" value="1"/>
            <enum name="solo" value="2"/>
        </attr>
    </declare-styleable>


</resources>